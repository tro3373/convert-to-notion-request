{
  "version": 2,
  "_installCommand": "npm install",
  "_buildCommand": "npm run build",
  "_outputDirectory": "dist",
  "_builds": [
    {
      "src": "dist/index.js",
      "use": "@vercel/node",
      "config": {
        "includeFiles": [
          "dist/**"
        ]
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "dist/index.js"
    }
  ]
}
